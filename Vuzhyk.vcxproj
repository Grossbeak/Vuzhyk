    </ItemGroup>
    <Copy SourceFiles="@(QtDlls)" DestinationFolder="$(OutDir)" SkipUnchangedFiles="true" />
    <MakeDir Directories="$(OutDir)platforms" />
    <Copy SourceFiles="$(QTDIR)\plugins\platforms\qwindowsd.dll" DestinationFolder="$(OutDir)platforms" SkipUnchangedFiles="true" Condition="Exists('$(QTDIR)\plugins\platforms\qwindowsd.dll')" />
    <Copy SourceFiles="$(QTDIR)\plugins\platforms\qwindows.dll" DestinationFolder="$(OutDir)platforms" SkipUnchangedFiles="true" Condition="!Exists('$(QTDIR)\plugins\platforms\qwindowsd.dll') AND Exists('$(QTDIR)\plugins\platforms\qwindows.dll')" />
  </Target>
  <Target Name="CopyQtDllsRelease" AfterTargets="Build" Condition="'$(Configuration)'=='Release'">
    <ItemGroup>
      <QtDlls Include="$(QTDIR)\bin\Qt5Core.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Gui.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Widgets.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Svg.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Network.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5PrintSupport.dll" Condition="Exists('$(QTDIR)\bin\Qt5PrintSupport.dll')" />
      <QtDlls Include="$(QTDIR)\bin\qscintilla2_qt5.dll" />
    </ItemGroup>
    <Copy SourceFiles="@(QtDlls)" DestinationFolder="$(OutDir)" SkipUnchangedFiles="true" />
    <MakeDir Directories="$(OutDir)platforms" />
    <Copy SourceFiles="$(QTDIR)\plugins\platforms\qwindows.dll" DestinationFolder="$(OutDir)platforms" SkipUnchangedFiles="true" Condition="Exists('$(QTDIR)\plugins\platforms\qwindows.dll')" />
  </Target>
  <!-- Копирование FilePicker.exe для Release -->
  <Target Name="CopyFilePickerRelease" AfterTargets="CopyQtDllsRelease" Condition="'$(Configuration)'=='Release'">
    <PropertyGroup>
      <FilePickerSource>$(SolutionDir)$(Platform)\$(Configuration)\FilePicker\FilePicker.exe</FilePickerSource>
    </PropertyGroup>
    <Copy SourceFiles="$(FilePickerSource)" 
          DestinationFolder="$(OutDir)" 
          SkipUnchangedFiles="true"
          Condition="Exists('$(FilePickerSource)')" />
    <Message Text="FilePicker.exe copied to $(OutDir)" Condition="Exists('$(FilePickerSource)')" Importance="normal" />
    <Warning Text="FilePicker.exe not found at $(FilePickerSource). Build FilePicker project first!" Condition="!Exists('$(FilePickerSource)')" />
  </Target>
  <!-- Копирование Python для Release -->
  <Target Name="CopyPythonRelease" AfterTargets="CopyFilePickerRelease" Condition="'$(Configuration)'=='Release'">
    <PropertyGroup>
      <PythonSourceDir Condition="Exists('$(SolutionDir)..\vuzhyk\dist\python')">$(SolutionDir)..\vuzhyk\dist\python</PythonSourceDir>
      <PythonSourceDir Condition="!Exists('$(SolutionDir)..\vuzhyk\dist\python') AND Exists('C:\Python38')">C:\Python38</PythonSourceDir>
      <PythonSourceDir Condition="!Exists('$(SolutionDir)..\vuzhyk\dist\python') AND !Exists('C:\Python38') AND Exists('C:\Python39')">C:\Python39</PythonSourceDir>
      <PythonSourceDir Condition="!Exists('$(SolutionDir)..\vuzhyk\dist\python') AND !Exists('C:\Python38') AND !Exists('C:\Python39') AND Exists('C:\Python310')">C:\Python310</PythonSourceDir>
      <PythonDestDir>$(OutDir)python</PythonDestDir>
    </PropertyGroup>
    <MakeDir Directories="$(PythonDestDir)" Condition="!Exists('$(PythonDestDir)')" />
    <!-- Копируем исполняемые файлы Python -->
    <ItemGroup>
      <PythonExes Include="$(PythonSourceDir)\python.exe" Condition="Exists('$(PythonSourceDir)\python.exe')" />
      <PythonExes Include="$(PythonSourceDir)\pythonw.exe" Condition="Exists('$(PythonSourceDir)\pythonw.exe')" />
    </ItemGroup>
    <Copy SourceFiles="@(PythonExes)" 
          DestinationFolder="$(PythonDestDir)" 
          SkipUnchangedFiles="true"
          Condition="'$(PythonSourceDir)' != ''" />
    <!-- Копируем Python DLL -->
    <ItemGroup>
      <PythonDlls Include="$(PythonSourceDir)\python*.dll" Condition="Exists('$(PythonSourceDir)')" />
    </ItemGroup>
    <Copy SourceFiles="@(PythonDlls)" 
          DestinationFolder="$(PythonDestDir)" 
          SkipUnchangedFiles="true"
          Condition="'$(PythonSourceDir)' != ''" />
    <!-- Копируем папки Lib и DLLs рекурсивно -->
    <Exec Command="robocopy &quot;$(PythonSourceDir)\Lib&quot; &quot;$(PythonDestDir)\Lib&quot; /E /NFL /NDL /NJH /NJS /NP" 
          Condition="'$(PythonSourceDir)' != '' AND Exists('$(PythonSourceDir)\Lib')"
          IgnoreExitCode="true" />
    <Exec Command="robocopy &quot;$(PythonSourceDir)\DLLs&quot; &quot;$(PythonDestDir)\DLLs&quot; /E /NFL /NDL /NJH /NJS /NP" 
          Condition="'$(PythonSourceDir)' != '' AND Exists('$(PythonSourceDir)\DLLs')"
          IgnoreExitCode="true" />
    <Message Text="Python copied from $(PythonSourceDir) to $(PythonDestDir)" Condition="'$(PythonSourceDir)' != ''" Importance="normal" />
    <Warning Text="Python directory not found. Please ensure Python is installed or available at $(SolutionDir)..\vuzhyk\dist\python" Condition="'$(PythonSourceDir)' == ''" />
  </Target>
  <!-- Копирование дополнительных Qt плагинов для Release -->
  <Target Name="CopyQtPluginsRelease" AfterTargets="CopyPythonRelease" Condition="'$(Configuration)'=='Release'">
    <PropertyGroup>
      <QtPluginsSource>$(QTDIR)\plugins</QtPluginsSource>
      <QtPluginsDest>$(OutDir)</QtPluginsDest>
    </PropertyGroup>
    <!-- Icon engines -->
    <MakeDir Directories="$(QtPluginsDest)iconengines" Condition="Exists('$(QtPluginsSource)\iconengines')" />
    <Copy SourceFiles="$(QtPluginsSource)\iconengines\qsvgicon.dll" 
          DestinationFolder="$(QtPluginsDest)iconengines" 
          SkipUnchangedFiles="true"
          Condition="Exists('$(QtPluginsSource)\iconengines\qsvgicon.dll')" />
    <!-- Image formats (только Release версии, без Debug) -->
    <MakeDir Directories="$(QtPluginsDest)imageformats" Condition="Exists('$(QtPluginsSource)\imageformats')" />
    <ItemGroup>
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qgif.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qgif.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qicns.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qicns.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qico.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qico.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qjpeg.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qjpeg.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qsvg.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qsvg.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qtga.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qtga.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qtiff.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qtiff.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qwbmp.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qwbmp.dll')" />
      <ImageFormats Include="$(QtPluginsSource)\imageformats\qwebp.dll" Condition="Exists('$(QtPluginsSource)\imageformats\qwebp.dll')" />
    </ItemGroup>
    <Copy SourceFiles="@(ImageFormats)" 
          DestinationFolder="$(QtPluginsDest)imageformats" 
          SkipUnchangedFiles="true" />
    <!-- Styles (только Release версии, без Debug) -->
    <MakeDir Directories="$(QtPluginsDest)styles" Condition="Exists('$(QtPluginsSource)\styles')" />
    <ItemGroup>
      <Styles Include="$(QtPluginsSource)\styles\qwindowsvistastyle.dll" Condition="Exists('$(QtPluginsSource)\styles\qwindowsvistastyle.dll')" />
    </ItemGroup>
    <Copy SourceFiles="@(Styles)" 
          DestinationFolder="$(QtPluginsDest)styles" 
          SkipUnchangedFiles="true" />
    <!-- Translations (опционально) -->
    <MakeDir Directories="$(QtPluginsDest)translations" Condition="Exists('$(QTDIR)\translations')" />
    <ItemGroup>
      <Translations Include="$(QTDIR)\translations\qt_*.qm" Condition="Exists('$(QTDIR)\translations')" />
    </ItemGroup>
    <Copy SourceFiles="@(Translations)" 
          DestinationFolder="$(QtPluginsDest)translations" 
          SkipUnchangedFiles="true" />
    <Message Text="Qt plugins copied to $(QtPluginsDest)" Importance="normal" />
  </Target>
  <!-- Очистка промежуточных файлов из выходной папки Release -->
  <Target Name="CleanIntermediateFiles" AfterTargets="CopyQtPluginsRelease" Condition="'$(Configuration)'=='Release'">
    <!-- Удаляем промежуточные файлы сборки -->
    <ItemGroup>
      <FilesToDelete Include="$(OutDir)*.obj" />
      <FilesToDelete Include="$(OutDir)*.pdb" Condition="'$(OutDir)' != ''" />
      <FilesToDelete Include="$(OutDir)*.iobj" />
      <FilesToDelete Include="$(OutDir)*.ipdb" />
      <FilesToDelete Include="$(OutDir)*.res" />
      <FilesToDelete Include="$(OutDir)*.tlog" />
      <FilesToDelete Include="$(OutDir)*.log" />
      <FilesToDelete Include="$(OutDir)*.recipe" />
      <FilesToDelete Include="$(OutDir)*.vcxproj.*" />
      <FilesToDelete Include="$(OutDir)qt\**\*" />
      <FilesToDelete Include="$(OutDir)Vuzhyk.tlog\**\*" />
    </ItemGroup>
    <Delete Files="@(FilesToDelete)" ContinueOnError="true" />
    <RemoveDir Directories="$(OutDir)qt" ContinueOnError="true" Condition="Exists('$(OutDir)qt')" />
    <RemoveDir Directories="$(OutDir)Vuzhyk.tlog" ContinueOnError="true" Condition="Exists('$(OutDir)Vuzhyk.tlog')" />
    <RemoveDir Directories="$(OutDir)obj" ContinueOnError="true" Condition="Exists('$(OutDir)obj')" />
    <!-- Удаляем Debug версии плагинов (файлы с суффиксом 'd') -->
    <ItemGroup>
      <DebugPlugins Include="$(OutDir)imageformats\*d.dll" />
      <DebugPlugins Include="$(OutDir)styles\*d.dll" />
    </ItemGroup>
    <Delete Files="@(DebugPlugins)" ContinueOnError="true" />
    <Message Text="Intermediate files and Debug plugins cleaned from $(OutDir)" Importance="normal" />
  </Target>
  <ImportGroup Label="ExtensionTargets">
  </ImportGroup>
