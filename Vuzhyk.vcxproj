      <QtDlls Include="$(QTDIR)\bin\Qt5Cored.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Guid.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Widgetsd.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Networkd.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Svgd.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5Networkd.dll" />
      <QtDlls Include="$(QTDIR)\bin\Qt5PrintSupportd.dll" Condition="Exists('$(QTDIR)\bin\Qt5PrintSupportd.dll')" />
